{
  "engines": {
    "node": ">= 0.12"
  },
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "rules": {
    "indent": [
      "error",
      "tab"
    ],
    "no-var": 0,
    "vars-on-top": 0,
    "comma-dangle": 0,
    "no-console": 0,
    "prefer-template": 0,
    "consistent-return": 0,
    "prefer-arrow-callback": 0,
    "object-shorthand": 0
  }
}